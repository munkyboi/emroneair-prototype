<div class='row'>
  <div class='col-md-6'>
    <div class="form-group">
      <label for="new-form-date">Select a date</label>
      <div class='input-field dateselect dateselect-current'>
        <a class="dateselect-icon" href="#">
          <i class='mdi mdi-calendar-search'></i>
        </a>
        <input id='new-form-date' type="text" class="form-control current" data-toggle='datepicker' aria-label="Search">
      </div>
    </div>
  </div>
  <div class='col-md-6'>
    <div class="form-group">
      <label for="new-form-time">Select a time</label>
      <div class='input-field dateselect dateselect-current'>
        <a class="dateselect-icon" href="#">
          <i class='mdi mdi-clock-time-eight-outline'></i>
        </a>
        <input id='new-form-time' type="text" class="form-control current" data-toggle='timepicker' aria-label="Search">
      </div>
    </div>
  </div>
  <div class="col-md-12">
    <div class='detail-box row'>
      <div class='content-title col-md-12'>
        <h5>Details</h5>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-BP">BP</label>
          <input name='BP' id="new-form-BP" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-Wt">Wt</label>
          <input name='Wt' id="new-form-Wt" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-Height">Height</label>
          <input name='Height' id="new-form-Height" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-OutputTotal">Output Total</label>
          <input name='OutputTotal' id="new-form-OutputTotal" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-Temp">Temp</label>
          <input name='Temp' id="new-form-Temp" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-TempMax">Temp Max</label>
          <input name='TempMax' id="new-form-TempMax" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-P">P</label>
          <input name='P' id="new-form-P" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-Resp">Resp</label>
          <input name='Resp' id="new-form-Resp" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-SaO2">SaO2</label>
          <input name='SaO2' id="new-form-SaO2" class="form-control">
        </div>
      </div>
      <div class='col-md-4'>
        <div class="form-group">
          <label for="new-form-IntakeIV">Intake IV</label>
          <input name='IntakeIV' id="new-form-IntakeIV" class="form-control">
        </div>
      </div>
    </div>
  </div>
</div>